//bind_events @1-B81214A6
function bind_events() {
    if (document.forms["fiscompras"]) check_and_bind('document.forms["fiscompras"]','onLoad',page_fiscompras_OnLoad);
    if (document.forms["fiscompras"]) check_and_bind('document.forms["fiscompras"].Button_Delete','onclick',page_fiscompras_Button_Delete_OnClick);
    if (document.forms["fiscompras"]) check_and_bind('document.forms["fiscompras"].Button2','onclick',page_fiscompras_Button2_OnClick);
    if (document.forms["fiscompras"]) check_and_bind('document.forms["fiscompras"].Button_Cancel','onclick',page_fiscompras_Button_Cancel_OnClick);
    forms_onload();
}
//End bind_events

window.onload = bind_events; //Assign bind_events @1-19F7B649
