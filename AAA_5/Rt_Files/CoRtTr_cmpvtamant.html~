function fInicializa(){
	with(document.fiscompras){
				if(getFromurl("ID",0)>0){
					slUrl="../In_Files/InTrTr_Cabe.php?pCabecera=false&com_RegNumero=" + hdRegNumero.value + 
					"&CoTrTr_detallePageSize=8&pTipoComp=" + hdTipoComp.value +  "&pNumComp=" + hdNumComp.value + 
					"#bkInicio";
		 		window.frames["frDetal"].location.replace(slUrl);
			}
	    }
	initTabs(Array('<strong>D</strong>ATOS IVA / ICE','<strong>R</strong>ETENCIONES', 
					'DATOS <strong>G</strong>ENERALES', 
					'<strong>I</strong>MPORT /EXPORT', 
					'<strong>C</strong>ONTABILIDAD' ),
					getCookie("coTabActive", -1),900,250, false,
					Array('D','R','F','G', 'I', 'C'));
fValidaRet()
}
